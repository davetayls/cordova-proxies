{
  "name": "cordova_proxies",
  "version": "1.0.12",
  "homepage": "https://github.com/davetayls/cordova_proxies",
  "authors": [
    "davetayls <dave@the-taylors.org>"
  ],
  "description": "Proxy libraries for working with device capabilities in cordova",
  "main": [
    "src/**/*.js",
    "src/**/*.js.map",
    "src/**/*.d.ts",
    "src/**/*.hbs",
    "src/**/*.json",
    "src/**/*.less",
    "src/**/*.css"
  ],
  "moduleType": [
    "amd"
  ],
  "keywords": [
    "cordova"
  ],
  "license": "MIT",
  "ignore": [
    "**/.*",
    "bower_components",
    "grunt",
    "Gruntfile.*",
    "node_modules",
    "src/**/*.ts",
    "src/**/*.map",
    "test",
    "tests"
  ],
  "dependencies": {
    "backbone": "~1.1.2",
    "jquery": "~2.1.1"
  },
  "typescript": {
    "definition": "cordova_proxies.d.ts"
  }
}
